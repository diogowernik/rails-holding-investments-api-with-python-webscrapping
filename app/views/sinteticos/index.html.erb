<p id="notice"><%= notice %></p>

<h1>Sinteticos</h1>

<table>
  <thead>
    <tr>
      <th>Carteira</th>
      <th>Ano</th>
      <th>Jan</th>
      <th>Fev</th>
      <th>Mar</th>
      <th>Abr</th>
      <th>Mai</th>
      <th>Jun</th>
      <th>Jul</th>
      <th>Ago</th>
      <th>Set</th>
      <th>Out</th>
      <th>Nov</th>
      <th>Dez</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sinteticos.each do |sintetico| %>
      <tr>
        <td><%= sintetico.carteira %></td>
        <td><%= sintetico.ano %></td>
        <td><%= sintetico.jan %></td>
        <td><%= sintetico.fev %></td>
        <td><%= sintetico.mar %></td>
        <td><%= sintetico.abr %></td>
        <td><%= sintetico.mai %></td>
        <td><%= sintetico.jun %></td>
        <td><%= sintetico.jul %></td>
        <td><%= sintetico.ago %></td>
        <td><%= sintetico.set %></td>
        <td><%= sintetico.out %></td>
        <td><%= sintetico.nov %></td>
        <td><%= sintetico.dez %></td>
        <td><%= link_to 'Show', sintetico %></td>
        <td><%= link_to 'Edit', edit_sintetico_path(sintetico) %></td>
        <td><%= link_to 'Destroy', sintetico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sintetico', new_sintetico_path %>
