<div class="float-right"><%= link_to new_admin_year_path do %>
	<button class="btn btn-outline-success">
		<em class="fa fa-plus-circle"></em> &nbsp; Novo Year
	</button><% end %>
</div>
<h3 class="h3-custom mb-4" id="minhas-paginas">
	<em class="fa fa-vcard-o text-success"></em> &nbsp; years
</h3>
<div class="table-responsive mt-2">
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>Year</th>
          <th>Url</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @years.each do |year| %>
          <tr>
            <td><%= year.title %></td>
            <td><%= year.slug %></td>
            <td> <%= link_to 'Edit', edit_admin_year_path(year.id) %> | <%= link_to 'Destroy', admin_year_path(year.id), method: :delete %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>

